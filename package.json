{
  "name": "soundplayer",
  "version": "1.0.0",
  "private": true,
  "author": "terenty rezman",
  "license": "ISC",
  "description": "",
  "homepage": "./",
  "main": "build/electron.js",
  "build": {
    "appId": "some.app.id",
    "files": [
      "build/**/*",
      "!build/sounds",
      "!build/settings.json"
    ],
    "extraFiles": [
      {
        "from": "build/sounds",
        "to": "sounds"
      },
      {
        "from": "build/settings.json",
        "to": "settings.json"
      }
    ],
    "win": {},
    "linux": {
      "target": "deb",
      "maintainer": "terenty.rezman@gmail.com"
    }
  },
  "scripts": {
    "electron": "electron .",
    "build": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "start": "webpack --mode=development && concurrently \"webpack-dev-server --mode=development\" \"electron .\"",
    "pack": "npm run prod && electron-builder --dir",
    "dist": "npm run prod && electron-builder"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "antd": "^3.25.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0",
    "file-loader": "^6.2.0",
    "howler": "^2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "debounce": "^1.2.1"
  }
}
